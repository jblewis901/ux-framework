	<h2>Animation Modifiers</h2>
	<div class="row">
		<div class="col-sm">
			<h4>Spin</h4>
			<div class="form-inline">
				<button class="btn btn-primary mx-2"><span class="ug ug-gear" ug-fx="spin"></span></button>
				<button class="btn btn-danger mx-2"><span class="ug ug-gear ug-spin"></span></button>
			</div>
		</div>
		<div class="col-sm">
			<h4>Pulse</h4>
			<div class="form-inline">
				<button class="btn btn-primary mx-2"><span class="ug ug-save" ug-fx="pulse"></span></button>
				<button class="btn btn-danger mx-2"><span class="ug ug-save ug-pulse"></span></button>
			</div>
		</div>
	</div>
