		<button type="button" class="ml-2 btn btn-sm btn-primary"><span class="ug ug-arrow-right"></span></button>
		<div class="ml-2 input-group input-group-sm">
			<div class="input-group-prepend">
				<span class="input-group-text">Alert</span>
			</div>
			<input type="text" class="form-control" placeholder="Message" aria-label="Message Text">
			<div class="input-group-append">
				<button class="btn btn-secondary"><span class="ug ug-gear"></span></button>
				<button class="btn btn-danger"><span class="ug ug-trash"></span></button>
				<button class="btn btn-success"><span class="ug ug-save"></span></button>
			</div>
		</div>
	</div>
	<div class="form-inline mb-2">
		<div class="btn-group mr-2">
			<button type="button" class="btn btn-primary">Button and Dropdown</button>
			<button type="button" class="btn btn-primary dropdown-toggle dropdown-toggle-split" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"> <span class="ug ug-gear" </button>
