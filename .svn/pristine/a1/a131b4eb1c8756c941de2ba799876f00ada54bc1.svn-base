<style>
	.ug-preview { margin: 0 auto; }
	.ug-preview ul { list-style: none; padding-left: 0; }
	.ug-preview li {
		background: #ededed;
		border: 1px solid white;
		border-radius: 8px;
		cursor: normal;
		float: left;
		font-size: 2rem;
		height: 100px;
		line-height: 1.4;
		padding: 10px;
		text-align: center;
		width: 12.5%;
		transition: all 0.2s ease;
	}
	.ug-preview li.inactive {
		opacity: 0.5;
	}
	.ug-preview li:not(.inactive):hover {
		background: #d6d6d6;
		color: #333366;
		cursor: pointer;
	}
	.ug-preview li div {
		font-family: "Lucida Console", Monaco, monospace;
		font-size: 0.35em;
	}
	@-webkit-keyframes click_animation {
		from { background: #333366; }
		to { background: #d6d6d6; }
	}
	.user-select {
		animation: click_animation 0.5s ease;
	}
	.check-finished {
		background: yellow;
	}
</style>
</head>

